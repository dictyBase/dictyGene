% layout 'default', title => 'Replaced record' ;

<div class="wrapper threecol-wide">
    <div id="subnav">
        <div class="inner_container"></div>
    </div>
    <div class="wrapper-left-center">
      <div class="wrapper-columns">
        <div class="column-center">
          <div id="column-center-content">
						<div class="info"> 
						<p>
							<b><%= $id %></b> has been deleted from dictyBase.
							It has been replaced by 
								% for my $rep(@$replaced) {
									<a href="/gene/<%= $rep %>"><%= $rep %></a> &nbsp;
								% }
						</p>
						</div>						
          </div>
				</div>
				<div class="column-left">
				  <div id="column-left-content"></div>
				</div>
				<div class="column-right">
		  </div>
		</div>
</div>



